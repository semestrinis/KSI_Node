extends layout

block content
	div(class="container")
		div(class="row")
			div(class="col-md-8")
				h1= title
		div(class="row move-down")
			div(class="col-md-8")
				a(href="/", class="btn btn-primary", role="button")
					span(class="glyphicon glyphicon-list")
					|  Grįžti
		div(class="row move-down")
			table.table
				thead
					tr
					th(scope='col') Temperatūra
					th(scope='col') Slėgis
					th(scope='col') Drėgnis
					th(scope='col') Šviesa
					th(scope='col') Data
				tbody
					tr
						each val in data.results
							td= val.Temperatura1
							td= val.Slegis
							td= val.Dregme
							td= val.Sviesa
							td= val.Data
							tr


			