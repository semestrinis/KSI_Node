extends layout

block content
	div(class="container")
		div(class="row")
			div(class="col-md-8")
				h1= title
		div(class="row move-down")
			div(class="col-md-8")
				a(href="/dataView", class="btn btn-primary", role="button")
					span(class="glyphicon glyphicon-list")
					|  Stebėjimų duomenys
				a(href="/aboutUs", class="btn btn-primary", role="button")
					span(class="glyphicon glyphicon-plus")
					|  Apie projektą
				a(href="https://github.com/semestrinis/KSI_Node", class="btn btn-primary", role="button")
					span(class="glyphicon glyphicon-plus")
					|  Šaltinio kodas
		h1	Dabartiniai duomenys
		div(class="row move-down")
			div(class="col-md-8")
				table.table
					thead
						tr
						th(scope='col') Temperatūra
						th(scope='col') Slėgis
						th(scope='col') Drėgnis
						th(scope='col') Šviesa
						th(scope='col') Laikas
					tbody
						tr
						th(scope='row') 1
						td Mark
						td Otto
						td @mdo
						td 12:45:00
						tr
		h1	Dabartinės ribos
		div(class="row move-down")
			div(class="col-md-8")
				table.table
					thead
						tr
						th(scope='col') Nuo
						th(scope='col') Iki
					tbody
						tr
						th(scope='row') 1
						td Mark
						tr